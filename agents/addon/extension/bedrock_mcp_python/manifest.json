{
    "type": "extension",
    "name": "bedrock_mcp_python",
    "version": "0.1.0",
    "language": "python",
    "dependencies": [
        {
            "type": "system",
            "name": "rte_runtime_python",
            "version": "0.4.0"
        }
    ],
    "api": {
        "property": {
            "api_key": {
                "type": "string"
            },
            "base_url": {
                "type": "string"
            },
            "greeting": {
                "type": "string"
            },
            "max_memory_length": {
                "type": "int64"
            },
            "max_tokens": {
                "type": "int64"
            },
            "model": {
                "type": "string"
            },
            "prompt": {
                "type": "string"
            },
            "temperature": {
                "type": "float64"
            },
            "top_p": {
                "type": "float64"
            },
            "top_k": {
                "type": "float64"
            },
            "mcp_server_ids": {
                "type": "string"
            }
        },
        "data_in": [
            {
                "name": "text_data",
                "property": {
                    "text": {
                        "type": "string"
                    }
                }
            }
        ],
        "data_out": [
            {
                "name": "text_data",
                "property": {
                    "text": {
                        "type": "string"
                    }
                }
            }
        ],
        "cmd_in": [
            {
                "name": "flush"
            }
        ],
        "cmd_out": [
            {
                "name": "flush"
            }
        ]
    }
}